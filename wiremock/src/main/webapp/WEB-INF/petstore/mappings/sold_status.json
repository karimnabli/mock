{        
    "request": {                                    
        "method": "GET",                        
        "urlPath": "/v2/pet/findByStatus",
        "queryParameters": {
            "status": {
                "equalTo": "sold"
            }
        }
    },                                      
    "response": {                                   
        "status": 200,                          
        "bodyFileName": "sold_status.json",
        "headers": {
            "Content-Type": "application/json"
        }
    }                                               
}